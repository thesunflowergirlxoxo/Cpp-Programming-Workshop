#include <iostream>

using namespace std;

int main() 
{
int age;

cout<<"Enter your age: ";

cin>>age;

if(age>=18)
  {
	cout<<"You are an adult. You are eligible to vote!";
	}	

	else
	{
		cout<<"You are still a minor. You are not eligible to vote! Please wait for "<<18-age<<" more year(s) to vote!";
	}

  return 0; 
  }
